{
  "__inputs": [
    {
      "name": "DS_GRAPHITE",
      "label": "Graphite",
      "description": "Grafana Cloud Graphite data source",
      "type": "datasource",
      "pluginId": "graphite",
      "pluginName": "Graphite"
    },
    {
      "name": "DS_LOKI",
      "label": "Loki",
      "description": "Grafana Cloud Loki data source",
      "type": "datasource",
      "pluginId": "loki",
      "pluginName": "Loki"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "graphite", "name": "Graphite", "version": "1.0.0" },
    { "type": "datasource", "id": "loki", "name": "Loki", "version": "1.0.0" }
  ],
  "id": null,
  "uid": "greendrop-kpi",
  "title": "GreenDrop — Business KPIs",
  "description": "Métriques métier temps réel de la plateforme GreenDrop",
  "tags": ["greendrop", "kpi", "production"],
  "timezone": "Europe/Paris",
  "refresh": "5m",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Commandes — Total",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "targets": [
        { "target": "greendrop.orders.total", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 100 }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Commandes — Aujourd'hui",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "targets": [
        { "target": "greendrop.orders.today", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "orange" }
        }
      }
    },
    {
      "id": 3,
      "title": "Revenu Total (€)",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "targets": [
        { "target": "greendrop.orders.revenue.total", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyEUR",
          "color": { "mode": "fixed", "fixedColor": "green" }
        }
      }
    },
    {
      "id": 4,
      "title": "Revenu Aujourd'hui (€)",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "targets": [
        { "target": "greendrop.orders.revenue.today", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyEUR",
          "color": { "mode": "fixed", "fixedColor": "green" }
        }
      }
    },
    {
      "id": 5,
      "title": "Taux de livraison à temps (%)",
      "type": "gauge",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "targets": [
        { "target": "greendrop.orders.on_time_rate", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 70 },
              { "color": "green", "value": 90 }
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Disputes Ouvertes",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "targets": [
        { "target": "greendrop.disputes.open", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 15 }
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Commandes par statut",
      "type": "timeseries",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        { "target": "greendrop.orders.status.pending", "refId": "A" },
        { "target": "greendrop.orders.status.paid", "refId": "B" },
        { "target": "greendrop.orders.status.shipped", "refId": "C" },
        { "target": "greendrop.orders.status.delivered", "refId": "D" },
        { "target": "greendrop.orders.status.cancelled", "refId": "E" }
      ],
      "fieldConfig": {
        "overrides": [
          { "matcher": { "id": "byName", "options": "pending" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow" } }] },
          { "matcher": { "id": "byName", "options": "paid" }, "properties": [{ "id": "color", "value": { "fixedColor": "blue" } }] },
          { "matcher": { "id": "byName", "options": "shipped" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange" } }] },
          { "matcher": { "id": "byName", "options": "delivered" }, "properties": [{ "id": "color", "value": { "fixedColor": "green" } }] },
          { "matcher": { "id": "byName", "options": "cancelled" }, "properties": [{ "id": "color", "value": { "fixedColor": "red" } }] }
        ]
      }
    },
    {
      "id": 11,
      "title": "Revenu (évolution)",
      "type": "timeseries",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        { "target": "greendrop.orders.revenue.total", "refId": "A" },
        { "target": "greendrop.orders.revenue.today", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyEUR",
          "custom": { "fillOpacity": 20 }
        }
      }
    },
    {
      "id": 20,
      "title": "Utilisateurs — Total",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 12 },
      "targets": [
        { "target": "greendrop.users.total", "refId": "A" }
      ]
    },
    {
      "id": 21,
      "title": "Utilisateurs — Vérifiés",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 12 },
      "targets": [
        { "target": "greendrop.users.verified", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "green" }
        }
      }
    },
    {
      "id": 22,
      "title": "Nouveaux utilisateurs aujourd'hui",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 12 },
      "targets": [
        { "target": "greendrop.users.new_today", "refId": "A" }
      ]
    },
    {
      "id": 23,
      "title": "Chauffeurs connectés / occupés",
      "type": "bargauge",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 12 },
      "targets": [
        { "target": "greendrop.drivers.online", "refId": "A" },
        { "target": "greendrop.drivers.busy", "refId": "B" },
        { "target": "greendrop.drivers.total", "refId": "C" }
      ],
      "fieldConfig": {
        "overrides": [
          { "matcher": { "id": "byName", "options": "online" }, "properties": [{ "id": "color", "value": { "fixedColor": "green" } }] },
          { "matcher": { "id": "byName", "options": "busy" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange" } }] }
        ]
      }
    },
    {
      "id": 24,
      "title": "Vérifications KYC",
      "type": "piechart",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 12 },
      "targets": [
        { "target": "greendrop.verifications.pending", "refId": "A" },
        { "target": "greendrop.verifications.approved", "refId": "B" },
        { "target": "greendrop.verifications.rejected", "refId": "C" }
      ],
      "fieldConfig": {
        "overrides": [
          { "matcher": { "id": "byName", "options": "pending" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow" } }] },
          { "matcher": { "id": "byName", "options": "approved" }, "properties": [{ "id": "color", "value": { "fixedColor": "green" } }] },
          { "matcher": { "id": "byName", "options": "rejected" }, "properties": [{ "id": "color", "value": { "fixedColor": "red" } }] }
        ]
      }
    },
    {
      "id": 30,
      "title": "Boutiques actives",
      "type": "stat",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 16 },
      "targets": [
        { "target": "greendrop.shops.total", "refId": "A" }
      ]
    },
    {
      "id": 31,
      "title": "Évolution chauffeurs",
      "type": "timeseries",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 20 },
      "targets": [
        { "target": "greendrop.drivers.online", "refId": "A" },
        { "target": "greendrop.drivers.busy", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 15 }
        }
      }
    },
    {
      "id": 32,
      "title": "Évolution utilisateurs",
      "type": "timeseries",
      "datasource": { "type": "graphite", "uid": "${DS_GRAPHITE}" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 20 },
      "targets": [
        { "target": "greendrop.users.total", "refId": "A" },
        { "target": "greendrop.users.verified", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 15 }
        }
      }
    },
    {
      "id": 40,
      "title": "Trafic & Logs",
      "type": "row",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 26 },
      "panels": []
    },
    {
      "id": 41,
      "title": "Requêtes API / 5min",
      "type": "stat",
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 27 },
      "targets": [
        {
          "expr": "count_over_time({app=\"greendrop-admin\"} [5m])",
          "editorMode": "code",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "blue" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 42,
      "title": "Visiteurs uniques / 24h",
      "type": "stat",
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 27 },
      "targets": [
        {
          "expr": "count(sum by (sessionId) (count_over_time({app=\"greendrop-admin\"} | json | __error__=`` | type=`page_view` [$__auto])))",
          "editorMode": "code",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "purple" }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 43,
      "title": "Latence moyenne (ms)",
      "type": "gauge",
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 27 },
      "targets": [
        {
          "expr": "avg_over_time({app=\"greendrop-admin\"} | json | __error__=`` | duration > 0 | unwrap duration [5m])",
          "editorMode": "code",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "max": 2000,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 44,
      "title": "Taux d'erreur (%)",
      "type": "gauge",
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 27 },
      "targets": [
        {
          "expr": "sum(count_over_time({app=\"greendrop-admin\"} | json | __error__=`` | status >= 500 [5m])) / sum(count_over_time({app=\"greendrop-admin\"} | json | __error__=`` | status > 0 [5m])) * 100",
          "editorMode": "code",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 45,
      "title": "Requêtes par endpoint",
      "type": "barchart",
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 31 },
      "targets": [
        {
          "expr": "sum by (path) (count_over_time({app=\"greendrop-admin\"} | json | __error__=`` [$__interval]))",
          "editorMode": "code",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 47,
      "title": "Pages vues",
      "type": "timeseries",
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 31 },
      "targets": [
        {
          "expr": "sum(count_over_time({app=\"greendrop-admin\"} | json | __error__=`` | type=`page_view` [$__auto]))",
          "editorMode": "code",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "green" },
          "custom": { "fillOpacity": 20 }
        }
      }
    },
    {
      "id": 46,
      "title": "Logs en temps réel",
      "type": "logs",
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 39 },
      "targets": [
        {
          "expr": "{app=\"greendrop-admin\"}",
          "editorMode": "code",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "wrapLogMessage": true
      }
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] },
  "schemaVersion": 39
}
